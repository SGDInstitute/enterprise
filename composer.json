{
  "name": "sgdinstitute/enterprise",
  "description": "SGD Institute Enterprise",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "barryvdh/laravel-cors": "^0.11.2",
    "barryvdh/laravel-dompdf": "^0.8.3",
    "digitalcloud/nova-resource-notes": "^1.4",
    "doctrine/dbal": "^2.6",
    "erusev/parsedown": "^1.7",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "^6.3",
    "hocza/sendy": "^1.1",
    "kabbouchi/nova-impersonate": "dev-master",
    "laracasts/flash": "^3.0",
    "laravel/framework": "5.8.*",
    "laravel/nova": "~2.0",
    "laravel/passport": "^7.4",
    "laravel/tinker": "^1.0",
    "maatwebsite/excel": "^3.1.8",
    "maatwebsite/laravel-nova-excel": "^1.1",
    "pragmarx/version": "^0.2.9",
    "sentry/sentry": "^1.7",
    "sentry/sentry-laravel": "^0.8.0",
    "sgd/checkin-queue": "*",
    "sgd/enterprise-admin-theme": "*",
    "sgd/form-builder": "*",
    "sgd/form-response": "*",
    "sgd/links": "*",
    "spatie/laravel-activitylog": "^3.5",
    "spatie/laravel-cors": "^1.6",
    "spatie/laravel-permission": "^2.6",
    "stripe/stripe-php": "^5.1",
    "titasgailius/search-relations": "^1.0",
    "vinkla/hashids": "^6.0",
    "vyuldashev/nova-permission": "^1.4"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^7.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    },
    {
      "type": "path",
      "url": "./nova-components/Links"
    },
    {
      "type": "path",
      "url": "./nova-components/FormBuilder"
    },
    {
      "type": "path",
      "url": "./nova-components/FormResponse"
    },
    {
      "type": "path",
      "url": "./nova-components/EnterpriseAdminTheme"
    },
    {
      "type": "path",
      "url": "./nova-components/CheckinQueue"
    },
    {
      "type": "path",
      "url": "./nova-components/CheckinQueue"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover",
      "@php artisan version:refresh"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
